package org.main.java;

public class Day2_2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String name1="Abhishek Pateriya";
		int empid1=3176;
		float salary1=50000.50f;
		String name2="Ankur Pateriya";
		int empid2=9410;
		float salary2=55999.59f;
		System.out.printf("%-20s%-10d%-10.2f\n",name1,empid1,salary1);
		System.out.printf("%-20s%-10d%-10.2f\n\n",name2,empid2,salary2);
		System.out.println(name1+" "+empid1+" "+salary1+"\n");
		System.out.printf("%s %d %f\n",name2,empid2,salary2);
		System.out.println("Sum : "+10+20);
		System.out.println("Sum : "+(10+20));

		

	}

}
